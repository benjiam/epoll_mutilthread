poll:
	gcc -O3 -g -c *.c -I../include -D_POLL
	ar -rc network.a *.o
	mv network.a ../	
poll-debug:
	gcc -g -c *.c -I../include -D_POLL
	ar -rc network.a *.o
	mv network.a ../
comp:
	gcc -O3 -g -c *.c -I../include -D
	ar -rc network.a *.o
	mv network.a ../	
comp-debug:
	gcc -g -c *.c -I../include -D
	ar -rc network.a *.o
	mv network.a ../	
